function showVideo() {
  console.log('[MOON] showVideo');

  $("#launcher_block").remove();
  $('#iframe_code_form').hide();
  $('#iframe_code_button').hide();
  $('#tech_works').hide();
  $('#eefdefecddfde').hide();

  removeAdv();

  // протестировать
