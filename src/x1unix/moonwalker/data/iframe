function postMessageReceive(event) {
  console.log("[MOON] postMessageReceive: ", event);

  if (event.origin == 'http://winvideo.org') {
    if (event.data == 'no_ads'){
      //sendStat(4, 'win_no_ads');
    }

    switch (event.data) {
      case 'tvzp_has_ad_0':
        //sendStat(4, 'mgg_no_ads');
        break;
      case 'tvzp_has_ad_1':
        //sendStat(4, 'mgg_ad_1');
        break;
      case 'tvzp_has_ad_2':
        //sendStat(4, 'mgg_ad_2');
        break;
    }
  }
}

$(function() {
  if (window.addEventListener) {
    window.addEventListener('message', postMessageReceive);
  } else {
    window.attachEvent('onmessage', postMessageReceive);
  }
});

